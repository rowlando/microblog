<ul class="feed">
  {% for item in pagination.items %}
    <li class="feed__item">
      <article class="flow">
        <h3>
          <a href="{{ item.url }}" rel="bookmark">{{ item.fileSlug | dateFilter }}</a>
        </h3>
        {{ item.templateContent | safe }}
      </article>
    </li>
  {% endfor %}
</ul>

